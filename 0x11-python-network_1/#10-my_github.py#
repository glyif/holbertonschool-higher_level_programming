:q#!/usr/bin/python3
"""
github auth
"""

import requests
from requests.auth import HTTPBasicAuth
from sys import argv


if __name__ == "__main__":
    try:
        username = sys.argv[1]
        password = sys.argv[2]
        url = 'https://api.github.com/user'
        r = requests.get(url, auth=HTTPBasicAuth(username, password))
        print("hi")
        print(r.json()['id'])
    except:
        print("None")
